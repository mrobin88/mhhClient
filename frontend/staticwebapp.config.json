{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/worker",
      "/worker/",
      "/worker/*",
      "/worker.html",
      "/assets/*",
      "*.css",
      "*.js",
      "*.png",
      "*.jpg",
      "*.svg",
      "*.ico",
      "*.woff",
      "*.woff2"
    ]
  },
  "routes": [
    {
      "route": "/worker",
      "serve": "/worker/index.html"
    },
    {
      "route": "/worker/",
      "serve": "/worker/index.html"
    },
    {
      "route": "/worker/*",
      "serve": "/worker/index.html"
    },
    {
      "route": "/worker.html",
      "serve": "/worker/index.html"
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript"
  }
}